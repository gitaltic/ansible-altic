<VirtualHost *:80>
ServerAdmin admin@example.com
DocumentRoot /home/{{ webuser }}/agrosmart/current/public
ErrorLog /var/log/apache2/error.log
LogLevel warn
CustomLog /var/log/apache2/access.log combined
<Directory "/home/{{ webuser }}/agrosmart/current/public">
Options -Indexes +FollowSymLinks
AllowOverride all
Require all granted
</Directory>
</VirtualHost